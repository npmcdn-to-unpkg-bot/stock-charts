{
  "name": "stock-charts",
  "version": "1.0.0",
  "description": "Interactive Stock Charting Module.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf -rf ./build",
    "watch": "node scripts/watch.js",
    "docs": "npm run build:docs && ncp docs/images/ build/images && ncp docs/data/ build/data && node scripts/copyhtml.js",
    "build": "npm run clean && npm run build:dev",
    "build:dev": "echo 'DEV -- BUILD' && webpack --config webpack.config.js --debug --progress --colors",
    "build:docs": "npm run build && echo 'DOCS - BUILD' && webpack --config webpack.config.docs.js --debug --progress --colors",
    "lint": "eslint --ext .js,.jsx src --fix",
    "test": "mocha --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nani3105/stock-charts.git"
  },
  "author": "Kurra Niranjan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nani3105/stock-charts/issues"
  },
  "homepage": "https://github.com/nani3105/stock-charts",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "jsdom": "^7.2.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.0.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.4.2",
    "prismjs": "^1.3.0",
    "react-addons-test-utils": "^0.14.3",
    "remarkable-loader": "^0.1.0",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "serve-static": "^1.10.0",
    "shelljs": "^0.5.3",
    "should": "^8.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "d3": "^3.5.10",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  }
}
